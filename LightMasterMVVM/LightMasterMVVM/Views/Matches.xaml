      <Setter Property="BorderThickness" Value="3,0,0,3"/>
      <Setter Property="Padding" Value="5"/>
    </Style>
    <Style Selector="Button.navbuttonselected2">
      <Setter Property="Background" Value="#2a7afa"/>
      <Setter Property="BorderBrush" Value="Black"/>
      <Setter Property="BorderThickness" Value="3,0,0,3"/>
      <Setter Property="Padding" Value="5"/>
    </Style>
    <Style Selector="Button.navbutton2/template/ContentPresenter">
      <Setter Property = "CornerRadius" Value = "6" />
    </Style>
    <Style Selector="Button:pointerover.navbutton2/template/ContentPresenter">
      <Setter Property = "BorderBrush" Value = "Black" />

      <Setter Property = "Background" Value = "#2c2c2c" />
    </Style>
